{
  "name": "@vite-asset-manager/nuxt",
  "version": "1.0.0",
  "type": "module",
  "description": "Nuxt module for vite-plugin-asset-manager",
  "author": "Ejiro Asiuwhu <ejiroasiuwhu10@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ejirocodes/vite-plugin-asset-manager#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ejirocodes/vite-plugin-asset-manager.git",
    "directory": "packages/nuxt"
  },
  "bugs": {
    "url": "https://github.com/ejirocodes/vite-plugin-asset-manager/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "vite",
    "assets",
    "asset-manager"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/module.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "prepack": "nuxt-module-build build"
  },
  "dependencies": {
    "@nuxt/kit": "^4.3.1",
    "@vite-asset-manager/core": "workspace:*",
    "h3": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.1",
    "nitropack": "^2.13.1",
    "nuxt": "^4.3.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "nuxt": ">=3.0.0"
  }
}
